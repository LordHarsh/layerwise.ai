{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "functions": {
    "py_api/index.py": {
      "runtime": "@vercel/python@6.1.6",
      "maxDuration": 60,
      "memory": 512,
      "includeFiles": "py_api/**/*.py",
      "excludeFiles": "{node_modules,py_api/venv,**/__pycache__,*.pyc,*.pyo,.git,.next,out,build,coverage,.vercel,resources,tests}/**"
    }
  }
}
